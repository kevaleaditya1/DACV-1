# Environment variables for Netlify deployment
# Copy these to Netlify's environment variables section

# Contract Configuration
REACT_APP_CONTRACT_ADDRESS=0x3753cfB00dd01D35A36284A909EcBb73a06Fcc7b
REACT_APP_CHAIN_ID=17000

# IPFS Configuration
# IMPORTANT: Get a valid API key from https://nft.storage
# REACT_APP_NFT_STORAGE_KEY=your_actual_nft_storage_api_key_here

# Temporary test mode (disable when you have a valid API key)
REACT_APP_IPFS_TEST_MODE=true

# Optional: Custom IPFS gateway
# REACT_APP_IPFS_GATEWAY=https://ipfs.io/ipfs

# Optional: Web3.Storage as alternative to NFT.Storage
# REACT_APP_WEB3_STORAGE_KEY=your_web3_storage_key_here